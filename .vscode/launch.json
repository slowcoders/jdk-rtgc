{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "RTGC",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/linux-x86_64-client-fastdebug/images/jdk/bin/java",
            "args": ["-XX:+UnlockExperimentalVMOptions",
                "-XX:+UseRTGC", 
                "-cp", "test/rtgc", "Main", "2", "1000"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build Main"
        },
        {
            "name": "Serail GC",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/linux-x86_64-client-fastdebug/images/jdk/bin/java",
            "args": ["-cp", "test/rtgc", "Main", "2", "1000"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build Main"
        }
    ]
}